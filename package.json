{
  "name": "daily-track-server",
  "version": "0.0.1",
  "description": "track stuff daily",
  "main": "server.js",
  "scripts": {
    "start:dev": "nodemon server.js",
    "build:lessold": "lessc ./src/less/style.less ./dist/css/style.css && copyfiles --flat ./src/css/*.css ./dist/css",
    "build:imgold": "copyfiles --flat ./src/img/* ./dist/img && copyfiles --flat ./src/favicon.ico ./dist",
    "build:bundleold": "browserify ./src/index.js -o ./dist/js/bundle.js && copyfiles --flat ./src/js/*.js ./dist/js",
    "build:cleanold": "rimraf ./dist/css/* ./dist/js/* ./dist/img/*",
    "watch:bundleold": "watchify ./src/index.js -o ./dist/js/bundle.js -v -d",
    "lint": "clear && jshint .",
    "test": "NODE_ENV=test nyc mocha",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "build": "npm run build:prod",
    "build:prod": "webpack --env production --colors --display-error-details --display-used-exports --display-modules",
    "build:dev": "webpack --env development --colors --display-error-details",
    "build:stats": "webpack --env production --profile --json > stats.json",
    "server:dev": "npm run test && npm run build:dev && node server.js",
    "server:prod": "npm run test && npm run build:prod && node server.js",
    "stats": "npm run build:stats",
    "stats:why": "whybundled stats.json",
    "stats:duplicates": "inspectpack -s stats.json -a duplicates",
    "stats:analyze": "webpack-bundle-analyzer stats.json ./dist",
    "stats:buddy": "bundle-buddy ./dist/*.map",
    "start": "node server.js",
    "deploy": "npm run test && npm run deploy:heroku",
    "deploy:heroku": "git push heroku master"
  },
  "author": "Kawika Heftel",
  "license": "ISC",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "@packtracker/webpack-plugin": "^2.2.0",
    "@ttskch/select2-bootstrap4-theme": "^1.2.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "bootstrap-confirmation2": "^4.1.0",
    "chart.js": "^2.8.0",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "express-webpack-assets": "^0.1.0",
    "hammerjs": "^2.0.8",
    "http-errors": "^1.7.2",
    "imports-loader": "^0.8.0",
    "jquery": "^3.4.0",
    "jshint": "^2.10.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-round": "^1.0.1",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "github:kheftel-lkw/passport-local-mongoose",
    "popper.js": "^1.15.0",
    "pug": "^2.0.3",
    "select2": "^4.0.8",
    "supports-color": "^7.0.0",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "browserify": "^16.2.3",
    "browserslist": "^4.5.5",
    "bundle-buddy": "^0.2.1",
    "clean-webpack-plugin": "^2.0.1",
    "copyfiles": "^2.1.0",
    "coveralls": "^3.0.6",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "inspectpack": "^4.2.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.11",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "should": "^13.2.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "uglify-js": "^3.5.4",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "watchify": "^3.11.1",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11",
    "whybundled": "^1.4.2"
  },
  "engines": {
    "node": "10.16.2"
  }
}
